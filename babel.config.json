{
  "presets": [
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    [
      "styled-jsx/babel",
      {
        "vendorPrefixes": false
      }
    ],
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css"
    }],
    "@babel/plugin-proposal-numeric-separator",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-proposal-optional-chaining"
  ]
}
